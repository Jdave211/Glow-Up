{
  "name": "looksmaxxing-server",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20 <26",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "migrate": "node src/db/migrate.js",
    "embeddings": "node src/embeddings/generate.js",
    "extract-images": "node src/embeddings/extract-images.js",
    "load-products": "node src/db/load-products-api.js",
    "doc2dataset": "node src/scripts/doc2dataset.js",
    "upload-finetune": "node src/scripts/upload-finetune.js",
    "scrape": "node src/scripts/scrape-ulta.js",
    "scrape:test": "node src/scripts/scrape-ulta.js --test 5",
    "scrape:resume": "node src/scripts/scrape-ulta.js --resume",
    "enrich": "node src/scripts/enrich-products.js",
    "enrich:test": "node src/scripts/enrich-products.js --test 5",
    "enrich:resume": "node src/scripts/enrich-products.js --resume",
    "embed": "node src/scripts/generate-embeddings.js",
    "embed:test": "node src/scripts/generate-embeddings.js --test 10",
    "embed:all": "node src/scripts/generate-embeddings.js --all",
    "product-embeds": "node src/scripts/finetune-product-embeds.js",
    "upload-product-embeds": "node src/scripts/upload-product-embeds.js"
  },
  "dependencies": {
    "@playwright/test": "^1.58.2",
    "@supabase/supabase-js": "^2.93.1",
    "@types/puppeteer": "^5.4.7",
    "apple-signin-auth": "^2.0.0",
    "body-parser": "^1.20.2",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "openai": "^6.17.0",
    "pg": "^8.17.2",
    "playwright": "^1.58.2",
    "puppeteer": "^24.37.1",
    "stripe": "^20.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
